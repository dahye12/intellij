package j09_클래스;

public class ProductMain {
    public static void main(String[] args) {

        Product p1 = new Product();
        p1.showInfo();

        Product p2 = new Product(10);
        p2.showInfo();

        Product p3 = new Product("박다혜");
        System.out.println();
        p3.productName = "닿";
        p3.showInfo(); // 또 다시 초기화하려면 또 다시 info 해줘야댐



        Product p4 = new Product(10,"박다혜");
        p3.showInfo();

    }
}
